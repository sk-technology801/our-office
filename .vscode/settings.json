{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control.bg3txt",
        "settings": {
          "foreground": "#C586C0"
        }
      },
      {
        "scope": "keyword.operator.bg3txt",
        "settings": {
          "foreground": "#ffa200"
        }
      },
      {
        "scope": "string.first-column.bg3txt",
        "settings": {
          "foreground": "#9CDCFE"
        }
      }
    ]
  },
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.page-template": "vue",
    "*.layout-template": "vue",
    "*.vue": "vue",
    "*.txt": "bg3txt"
  }
}
